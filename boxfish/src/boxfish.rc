#include <windows.h>

#include "boxfish_config.h"

boxfish                 ICON    "../../icons/logo-new-icon.ico"

// this stuff is from 
// http://msdn.microsoft.com/library/en-us/tools/tools/versioninfo_resource.asp


1 VERSIONINFO
  FILEVERSION    BOXFISH_COMMA_VERSION
  PRODUCTVERSION BOXFISH_COMMA_VERSION
  FILEFLAGSMASK  0x3fL
#ifdef _DEBUG
  FILEFLAGS VS_FF_DEBUG
#else
  FILEFLAGS 0x0L
#endif
  FILEOS VOS_NT_WINDOWS32
  FILETYPE VFT_APP
  FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "04090000"
        BEGIN
            VALUE "Comments", BOXFISH_DESCRIPTION "\0"
            VALUE "CompanyName", BOXFISH_CREATOR_NAME "\0"
            VALUE "FileDescription", BOXFISH_DESCRIPTION "\0"
            VALUE "FileVersion", BOXFISH_VERSION_STRING "\0"
            VALUE "InternalName", BOXFISH_APPNAME "\0"
            VALUE "LegalCopyright", BOXFISH_COPYRIGHT "\0"
            VALUE "LegalTrademarks", BOXFISH_COPYRIGHT "\0"
            VALUE "OriginalFilename", BOXFISH_APPNAME ".exe\0"
            VALUE "PrivateBuild", "\0"
            VALUE "ProductName", BOXFISH_DESCRIPTION "\0"
            VALUE "ProductVersion", BOXFISH_VERSION_STRING "\0"
            VALUE "SpecialBuild", "\0"
        END
    END
END


